{
  "name": "@devflow/sdk",
  "version": "1.0.0",
  "description": "SDK with abstractions for VCS, CI, Code Agents, and Project adapters",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "clean": "rm -rf dist *.tsbuildinfo",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": ["devflow", "sdk", "vcs", "ci", "agent"],
  "author": "DevFlow Team",
  "license": "MIT",
  "dependencies": {
    "@devflow/common": "workspace:*",
    "@notionhq/client": "^2.2.14",
    "@octokit/rest": "^20.0.2",
    "axios": "^1.6.5",
    "yaml": "^2.3.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.11.5",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  }
}
