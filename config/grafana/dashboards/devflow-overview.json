{
  "dashboard": {
    "title": "Soma Squad AI - Overview",
    "tags": ["soma-squad-ai", "overview"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Active Workflows",
        "type": "stat",
        "targets": [
          {
            "expr": "soma-squad-ai_active_workflows",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 4, "w": 6, "x": 0, "y": 0 }
      },
      {
        "id": 2,
        "title": "Workflow Success Rate (24h)",
        "type": "gauge",
        "targets": [
          {
            "expr": "rate(soma-squad-ai_workflow_executions_total{status=\"completed\"}[24h]) / rate(soma-squad-ai_workflow_executions_total[24h]) * 100",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 80, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 6, "y": 0 }
      },
      {
        "id": 3,
        "title": "LLM Daily Cost",
        "type": "stat",
        "targets": [
          {
            "expr": "soma-squad-ai_llm_cost_usd{period=\"day\"}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "currencyUSD",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 40, "color": "yellow" },
                { "value": 50, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 12, "y": 0 }
      },
      {
        "id": 4,
        "title": "Alerts (Last Hour)",
        "type": "stat",
        "targets": [
          {
            "expr": "increase(soma-squad-ai_alerts_total[1h])",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "green" },
                { "value": 5, "color": "yellow" },
                { "value": 10, "color": "red" }
              ]
            }
          }
        },
        "gridPos": { "h": 4, "w": 6, "x": 18, "y": 0 }
      },
      {
        "id": 5,
        "title": "Workflow Duration (P50/P95/P99)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(soma-squad-ai_workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "P50",
            "refId": "A"
          },
          {
            "expr": "histogram_quantile(0.95, rate(soma-squad-ai_workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "P95",
            "refId": "B"
          },
          {
            "expr": "histogram_quantile(0.99, rate(soma-squad-ai_workflow_duration_seconds_bucket[5m]))",
            "legendFormat": "P99",
            "refId": "C"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Duration" }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 4 }
      },
      {
        "id": 6,
        "title": "Agent Latency by Provider",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(soma-squad-ai_agent_latency_seconds_sum[5m]) / rate(soma-squad-ai_agent_latency_seconds_count[5m])",
            "legendFormat": "{{provider}} - {{phase}}",
            "refId": "A"
          }
        ],
        "yaxes": [
          { "format": "s", "label": "Latency" }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 4 }
      },
      {
        "id": 7,
        "title": "SLA Compliance",
        "type": "bargauge",
        "targets": [
          {
            "expr": "soma-squad-ai_sla_compliance_percent",
            "legendFormat": "{{sla_name}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percent",
            "min": 0,
            "max": 100,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                { "value": 0, "color": "red" },
                { "value": 90, "color": "yellow" },
                { "value": 95, "color": "green" }
              ]
            }
          }
        },
        "gridPos": { "h": 8, "w": 12, "x": 0, "y": 12 }
      },
      {
        "id": 8,
        "title": "Tickets by State",
        "type": "piechart",
        "targets": [
          {
            "expr": "soma-squad-ai_tickets_in_progress",
            "legendFormat": "{{state}}",
            "refId": "A"
          }
        ],
        "gridPos": { "h": 8, "w": 12, "x": 12, "y": 12 }
      }
    ],
    "refresh": "10s",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}



